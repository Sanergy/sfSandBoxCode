<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Trial_Checklist_Completed_not_empty</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
   ISPICKVAL( Trial_Checklist_Completed__c  , &quot;&quot;),
   ISPICKVAL( Trial_status__c   , &quot;Planted&quot;)
)</errorConditionFormula>
    <errorMessage>Please fill out this field if Trial Status is &quot;Panted&quot;</errorMessage>
</ValidationRule>
