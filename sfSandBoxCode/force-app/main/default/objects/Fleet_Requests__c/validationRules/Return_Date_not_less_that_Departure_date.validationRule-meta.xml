<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Return_Date_not_less_that_Departure_date</fullName>
    <active>true</active>
    <errorConditionFormula>Return_Date__c &lt;  Departure_Date__c</errorConditionFormula>
    <errorMessage>Return date cannot be less than departure date</errorMessage>
</ValidationRule>
