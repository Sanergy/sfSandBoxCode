<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_Date_not_larger_that_End_Date</fullName>
    <active>true</active>
    <errorConditionFormula>Actual_End_Date__c &lt;  Actual_Start_Date__c</errorConditionFormula>
    <errorMessage>Start Date cannot be larger than End Date</errorMessage>
</ValidationRule>
